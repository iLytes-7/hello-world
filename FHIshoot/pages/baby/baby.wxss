.zhe{
  position: fixed;
  top: 0;
  right: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0,0,0, 0.3);
  z-index: 9999
}
.zheBody{
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  width: 70%;
  height: 50%;
  background: white;
  
}
.zheMain{
  text-align: center;
  width: 80%;
  position: absolute;
  top: 50%;
  left: 50%;
