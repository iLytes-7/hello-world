.text {
  width: 16%;
  text-align: center;
  color: #CB8D23;
  font-family: "仿宋";
  font-weight: 600;
  font-size: 43rpx;
  position: relative
}
.text::before,.text::after {
  content: ''; /*CSS伪类用法*/
  position: absolute; /*定位背景横线的位置*/
  top: 83%;
  background: #F4A41A; /*宽和高做出来的背景横线*/
  width: 80rpx;
  height: 1px;
}
.text::before {
  left: -80rpx;
}
.text::after {
  right: -80rpx;
}
